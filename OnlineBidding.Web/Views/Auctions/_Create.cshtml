<h1>Create</h1>
<form id="createForm">
    <div class="form-inline">
        <label>Title</label>
        <input name="Title" class="form-control" />
    </div>
    <div class="form-inline">
        <label>PictureUrl</label>
        <input name="PictureUrl" class="form-control" />
    </div>

    <div class="form-inline">
        <label>Description</label>
        <input name="Description" class="form-control" />
    </div>
    <div class="form-inline">
        <label>ActualAmount</label>
        <input name="ActualAmount" class="form-control" />
    </div>
    <div class="form-inline">
        <label>StartingTime</label>
        <input name="StartingTime" class="form-control" />
    </div>
    <div class="form-inline">
        <label>EndingTime</label>
        <input name="EndingTime" class="form-control" />
    </div>

    
    <input type="button" class="btn btn-primary" id="saveBtn" value="Save" />
</form>

<script>
    $("#saveBtn").click(function () {
        $.ajax({
            url: "/Auctions/Create",
            method: "post",
            data: $("#createForm").serialize()


        }).done(function (response) {
            console.log(response);
            $("#tableDiv").html(response);
            $("#auctionForm").html("");
            });

    });
</script>