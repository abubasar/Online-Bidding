
<div class="modal-header">
    <h5 class="modal-title">Create Auction</h5>
    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
        <span aria-hidden="true">&times;</span>
    </button>
</div>
<form id="createForm">
  
    <div class="modal-body">
        <div class="form-inline">
            <label>Title</label>
            <input name="Title" class="form-control" />
        </div>
        <div class="form-inline">
            <label>PictureUrl</label>
            <input name="PictureUrl" class="form-control" />
        </div>

        <div class="form-inline">
            <label>Description</label>
            <input name="Description" class="form-control" />
        </div>
        <div class="form-inline">
            <label>ActualAmount</label>
            <input name="ActualAmount" class="form-control" />
        </div>
        <div class="form-inline">
            <label>StartingTime</label>
            <input name="StartingTime" class="form-control" />
        </div>
        <div class="form-inline">
            <label>EndingTime</label>
            <input name="EndingTime" class="form-control" />
        </div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <input type="button" class="btn btn-primary" id="saveBtn" value="Save" />
    </div>
</form>




<script>
    $("#saveBtn").click(function () {
        $.ajax({
            url:  "@Url.Action("Create","Auctions")",
            method: "post",
            data: $("#createForm").serialize()


        }).done(function (response) {
            console.log(response);
            $("#formModal").modal("toggle");
            $("#tableDiv").html(response);
            
        });

    });
</script>